/*
В предыдущих домашних заданиях найти блоки кода, которые возможно выделить
в функции и определить их в функции. Например, запись в файл, вычисление
формул.
*/
#include<iostream>
#include<cmath>
floatLike(intx)
{
floaty;

if(x>1)
y=7+(23*x);
elseif(x<1)
y=((x+2)*32)-7;
else
y=4;

std::cout«"Y:"«y;

returnx;
}
intmain()
{
floatx;

std::cout«"X:";
std::cin»x;
std::cout«Like(x)«std::endl;

return 0;
}
